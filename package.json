{
  "name": "cube-transform",
  "version": "1.0.0",
  "description": "Cube transformations",
  "main": "build/cube-transform.js",
  "scripts": {
    "build": "webpack && NODE_ENV=development webpack",
    "dev": "webpack-dev-server --inline --hot",
    "test": "karma start --single-run",
    "test:auto": "karma start",
    "docs": "rm -rf docs; documentation lint src/**; documentation build src/cube-transform.js -a public -f html -o docs -g --name 'Cube Transform'",
    "docs:serve" : "documentation serve src/cube-transform.js -a public -w -g --name 'Cube Transform'",
    "docs:lint": "documentation lint src/**"
  },
  "keywords": [
    "rubik's cube"
  ],
  "author": "ldez",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "documentation": "^4.0.0-beta.18",
    "dotenv": "^8.0.0",
    "isparta": "^4.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "phantomjs": "^2.1.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
